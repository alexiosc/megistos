msgid "User login handler"
msgstr "Αρχική είσοδος στο σύστημα (login)"

#: lib/bbsmod.c:693
msgid "User-interactive handler"
msgstr "Κανονική χρήση (interactive)"

#: lib/bbsmod.c:696
msgid "User logout handler"
msgstr "Αποσύνδεση από το σύστημα"

#: lib/bbsmod.c:699
