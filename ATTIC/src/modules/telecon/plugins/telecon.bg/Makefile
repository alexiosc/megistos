teleconpluginPROGRAMS_INSTALL = $(INSTALL_PROGRAM)
install-teleconpluginPROGRAMS: $(teleconplugin_PROGRAMS)
	@$(NORMAL_INSTALL)
	$(mkinstalldirs) $(DESTDIR)$(teleconplugindir)
	@list='$(teleconplugin_PROGRAMS)'; for p in $$list; do \
	  p1=`echo $$p|sed 's/$(EXEEXT)$$//'`; \
	  if test -f $$p \
	     || test -f $$p1 \
	  ; then \
	    f=`echo "$$p1" | sed 's,^.*/,,;$(transform);s/$$/$(EXEEXT)/'`; \
	   echo " $(INSTALL_PROGRAM_ENV) $(LIBTOOL) --mode=install $(teleconpluginPROGRAMS_INSTALL) $$p $(DESTDIR)$(teleconplugindir)/$$f"; \
	   $(INSTALL_PROGRAM_ENV) $(LIBTOOL) --mode=install $(teleconpluginPROGRAMS_INSTALL) $$p $(DESTDIR)$(teleconplugindir)/$$f; \
	  else :; fi; \
	done

uninstall-teleconpluginPROGRAMS:
	@$(NORMAL_UNINSTALL)
	@list='$(teleconplugin_PROGRAMS)'; for p in $$list; do \
	  f=`echo "$$p" | sed 's,^.*/,,;s/$(EXEEXT)$$//;$(transform);s/$$/$(EXEEXT)/'`; \
	  echo " rm -f $(DESTDIR)$(teleconplugindir)/$$f"; \
	  rm -f $(DESTDIR)$(teleconplugindir)/$$f; \
	done

clean-teleconpluginPROGRAMS:
	@list='$(teleconplugin_PROGRAMS)'; for p in $$list; do \
	  f=`echo $$p|sed 's/$(EXEEXT)$$//'`; \
	  echo " rm -f $$p $$f"; \
	  rm -f $$p $$f ; \
	done
