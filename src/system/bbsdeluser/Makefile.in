install-scriptmegistosbinSCRIPTS: $(scriptmegistosbin_SCRIPTS)
	@$(NORMAL_INSTALL)
	$(mkinstalldirs) $(DESTDIR)$(scriptmegistosbindir)
	@list='$(scriptmegistosbin_SCRIPTS)'; for p in $$list; do \
	  if test -f "$$p"; then d=; else d="$(srcdir)/"; fi; \
	  if test -f $$d$$p; then \
	    f=`echo "$$p" | sed 's|^.*/||;$(transform)'`; \
	    echo " $(scriptmegistosbinSCRIPT_INSTALL) $$d$$p $(DESTDIR)$(scriptmegistosbindir)/$$f"; \
	    $(scriptmegistosbinSCRIPT_INSTALL) $$d$$p $(DESTDIR)$(scriptmegistosbindir)/$$f; \
	  else :; fi; \
	done

uninstall-scriptmegistosbinSCRIPTS:
	@$(NORMAL_UNINSTALL)
	@list='$(scriptmegistosbin_SCRIPTS)'; for p in $$list; do \
	  f=`echo "$$p" | sed 's|^.*/||;$(transform)'`; \
	  echo " rm -f $(DESTDIR)$(scriptmegistosbindir)/$$f"; \
	  rm -f $(DESTDIR)$(scriptmegistosbindir)/$$f; \
	done

